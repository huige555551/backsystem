package operation.pojo.course;

public class UserLesson {
	private Object lesson; //课时
	private int lastProess;  //上次播放点（毫秒单位）
	private boolean studyed;//是否学习完毕
	public UserLesson(){
		super();
	}
	public UserLesson(String lessonId){
		super();
		this.lesson=lessonId;
		this.lastProess=0;
		this.studyed=false;
	}
	public Object getLesson() {
		return lesson;
	}
	public void setLesson(Object lesson) {
		this.lesson = lesson;
	}
	public int getLastProess() {
		return lastProess;
	}
	public void setLastProess(int lastProess) {
		this.lastProess = lastProess;
	}
	public boolean isStudyed() {
		return studyed;
	}
	public void setStudyed(boolean studyed) {
		this.studyed = studyed;
	}
	
	
	
}
