package operation.pojo.ad;

import org.springframework.data.annotation.Id;

/**
 * 
 * @ClassName: Ad
 * @Description: 广告位
 * @author tangli
 * @date 2015年3月17日 下午1:35:16
 *
 */
public class AdVo {
	private String adId;//广告位id
	private String name;//广告位名称
	private String adSid;//渠道商Id 
	private String creater;//创建人
	private String adSellerId;//渠道商Id号 
	private String adSellerName;//渠道商名称	
	private long adTime;
	private long ccount;//点击数量
	private long rcount;//注册数量

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAdSid() {
		return adSid;
	}

	public void setAdSid(String adSid) {
		this.adSid = adSid;
	}

	public String getAdSellerId() {
		return adSellerId;
	}

	public void setAdSellerId(String adSellerId) {
		this.adSellerId = adSellerId;
	}

	public String getAdSellerName() {
		return adSellerName;
	}

	public void setAdSellerName(String adSellerName) {
		this.adSellerName = adSellerName;
	}
	public String getCreater() {
		return creater;
	}

	public void setCreater(String creater) {
		this.creater = creater;
	}

	public long getCcount() {
		return ccount;
	}

	public void setCcount(long ccount) {
		this.ccount = ccount;
	}

	public long getRcount() {
		return rcount;
	}

	public void setRcount(long rcount) {
		this.rcount = rcount;
	}

	public String getAdId() {
		return adId;
	}

	public void setAdId(String adId) {
		this.adId = adId;
	}

	public long getAdTime() {
		return adTime;
	}

	public void setAdTime(long adTime) {
		this.adTime = adTime;
	}


}
