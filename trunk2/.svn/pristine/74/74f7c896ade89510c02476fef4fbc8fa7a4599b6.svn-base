package operation.pojo.index;

import java.util.List;

import net.sf.json.JSONObject;
import operation.pojo.ad.ZtiaoAd;
import operation.pojo.dynamic.GroupDynamic;


public class NewIndexBean {
	
	private Object groupDynamic;
	
	private JSONObject recommend;
	
	private List<ZtiaoAd> ztiaoAd;

	public Object getGroupDynamic() {
		return groupDynamic;
	}

	public void setGroupDynamic(Object groupDynamic) {
		this.groupDynamic = groupDynamic;
	}

	public JSONObject getRecommend() {
		return recommend;
	}

	public void setRecommend(JSONObject recommend) {
		this.recommend = recommend;
	}

	public List<ZtiaoAd> getZtiaoAd() {
		return ztiaoAd;
	}

	public void setZtiaoAd(List<ZtiaoAd> ztiaoAd) {
		this.ztiaoAd = ztiaoAd;
	}

}
